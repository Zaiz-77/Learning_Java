package PracticeDiary_1;
// if 语句的使用
public class code_2_maxTwo {
    public static void main(String[] args) {
        int a = 11 , b = 13 , c = 16;
        int temp;
        if (a > b){
            temp = a;
        }
        else {
            temp = b;
        }
        if (temp < c){
            temp = c;
        }
        System.out.println("三个值的最大值是：" + temp);
    }
}
