package PracticeDiary_1;
// 三元运算符的使用：
public class code_1_maxOne {
    public static void main(String[] args) {
        int a = 15;
        int b = 13;
        int c = 16;
        // 也可以 int a = 10, b = 15, c = 16; 这样子初始化变量
        int max_1 = (a > b)? a : b;
        int max_one = (b > c)? b : c;
        System.out.println(max_one);
    }
}
